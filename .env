# ==================================
# MongoDB Atlas Configuration
# Replace these with your actual credentials:
MONGODB_URI=mongodb+srv://<your-atlas-username>:<your-atlas-password>@<cluster-name>.mongodb.net/reporting_app?retryWrites=true&w=majority

# ==================================
# Google OAuth Credentials
GOOGLE_CLIENT_ID=your_actual_client_id
GOOGLE_CLIENT_SECRET=your_actual_client_secret

# ==================================
# Facebook OAuth Credentials
FACEBOOK_APP_ID=your_actual_app_id
FACEBOOK_APP_SECRET=your_actual_app_secret

# ==================================
# Session Configuration
SESSION_SECRET=your_secure_session_secret

# ==================================
# Server Configuration
PORT=3000